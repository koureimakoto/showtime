    document.getElementById("mybtn").addEventListener("click", () => {
    const result = Module.ccall(
        'printName', // name of C function
        null,   // return type
        ['string'],   // argument types
        ['Talles']    // arguments
    );})

    

    Module.onRuntimeInitilized = async () => {
        const api = {
            version: Module.cwrap('printName', null, ['string']),
        }
        api.printName('TAlles')
    }